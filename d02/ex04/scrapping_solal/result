<script>
    // form sort select result
    $(function()
    {
        $('ul#sortSelect li a').click(function()
        {
            var namesort = $(this).attr('name');
            var name = namesort.split('_')[0];
            var value = namesort.split('_')[1];

            //var data_url = document.URL.replace(/([\?||&]+sort[+[a-z]+])+=[a-z]{0,4}/g, '');
            var data_url = document.URL.replace(/(sort[+[a-z]+])+=[a-z]{0,4}[?&]/g, '');

            if(data_url.match(/(sort\[.{0,}\]+=.{0,})/))
            {
                if(name !== 'reset')
                {
                    data_url = data_url+'&sort['+name+']='+value;
                }
            }

            $('#formSearch').append('<input type="hidden" name="'+name+'" value="'+value+'">');

            var protocol = window.location.protocol;
            var host = window.location.hostname;
            var pathname = window.location.pathname;

            var controller = protocol+'//'+host+pathname+'sort/'+namesort+'?data_url='+data_url;

            //console.log(controller);

            $.ajax({
                type: "GET",
                url: controller,
                contentType: "application/x-www-form-urlencoded;charset=UTF-8",
                success: function(text)
                {
                    if(text)
                    {
                        $(location).attr('href', text);
                    }
                }
            });

        });
    });
</script>    </div>
</form>

