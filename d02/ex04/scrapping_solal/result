    <script type="text/javascript">
        $('document').ready(function() {
            function real_load_pagination() {
                if (document.getElementsByClassName) {
                    var pagination_exceed = document.getElementsByClassName("pagination_exceed");
                    for (var i = 0; i < pagination_exceed.length; i++) {
                        pagination_exceed[i].style.display = "none";
                    }
                    ;
                } else {
                    setTimeout(real_load_pagination, 200);
                }
            }

            setTimeout(real_load_pagination, 200);

            var pagination_display_exceed = function (link, num) {
                var this_parent = link.parentNode.parentNode;
                var pe = document.getElementsByClassName("pagination_exceed" + num, this_parent)[0];
                var pmv = document.getElementsByClassName("pagination_main_visible", this_parent)[0];
                if (pe.style.display == "none") {
                    pe.style.display = "inline";
                    pmv.style.display = "none";
                } else {
                    pe.style.display = "none";
                    pmv.style.display = "inline";
                }

                return false;
            };
            $(function () {
                                $('a.plus-de-critere').each(function () {
                    $(this).attr('href', 'http://www.hachette-vins.com/vins/page-62/list/?filtre%5Bedition%5D=2017&amp;filtre%5Bregion%5D=Bordelais&amp;filtre%5Bcouleur%5D=rouge');
                });
                $('span[class^=md_cryptlink]').each(function () {
                    var urlMdCrypt = $(this).attr('class');
                    urlMdCrypt = urlMdCrypt.replace('md_cryptlink', '');
                    $.base64.utf8encode = true;
                    $(this).wrap('<a href="' + $.base64.decode(urlMdCrypt) + '" />');
                });
            });
        });
    </script>
</div>
            
        </nav>
