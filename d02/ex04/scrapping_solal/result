            var couleur                  = ($('#criteres_couleur').data('value') == undefined) ? "-" : $('#criteres_couleur').data('value');
            var region                   = ($('#criteres_region').data('value') == undefined) ? "-" : $('#criteres_region').data('value');
            var prix                     = ($('#criteres_prix').data('value') == undefined) ? "-" : $('#criteres_prix').data('value');
            var note                     = ($('#criteres_note').data('value') == undefined) ? "-" : textNote[parseInt($('#criteres_note').data('value'))];
            var coupDeCoeur              = ($('#criteres_coeur').data('value') == undefined) ? "-" : $('#criteres_coeur').data('value');
